Menu : {
  SubCapa(yo,padre):{
    Elemento(data):{
      codigo: string,
      estado: string,
      contenido: string,
      nodo: DOM,
      enlace: URL,

      Metodos
        construirNodo()
    }
    estado: string,
    nodo: DOM,
    elementos: Array,
    codigo: string,
    padre: Privilegio,
    yo: Privilegio,
    hijos: array,

    Metodos
      construir()
      agregarElemento(contenido,enlace,selecionado)
      buscarElemento(codigo)
  }

  estado: string,
  capaActiva: SubCapa,
  partes: array, ----- titulo,pie : DOM
  nodo: DOM,
  intervaloCarga: numeric, ----- en desuso

  Metodos
    construir()
    abrirMenu()
    activarCapa(capa) ---- activa la capa recibida como parametro
    getEstado()
    avanzar(nodo) -------- moviento de capas hacia capa especificada en enlace de NODO recibido como parametro
    regresar() ----------- movimiento de capas en menu hacia capa anterior
    cambiarTitulo(texto) - cambial el texto del titulo del menu por el recibido
}
